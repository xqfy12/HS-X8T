      <monitor_task>game3</monitor_task>
    </Game3>
    <VideoOff level="0" constraint="CodaDefault" enable="1">
      <coda_info>state=0</coda_info>
    </VideoOff>
    <VideoPlayLow level="0" constraint="CodaPlaybackLow" enable="1">
